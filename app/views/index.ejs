        <div>
            <nav class="navbar navbar-default navbar-fixed-top">
                <div class="container">

                    <a class="navbar-brand title" href="#/">Services</a>    
                    <% if( logged ){ %> 
                    <ul class="nav navbar-nav navbar-right" data-ng-controller="MainController">
                        <li class="dropdown">
                          <a class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><i style="font-size:25px;" class="fa fa-shopping-cart"></i><span style="margin-top: -30px;color:#a94442;background-color:#dca7a7" class="badge">{{ qtde }}</span></a>
                            <ul class="dropdown-menu">
                                <li><a class="menu-navbar" data-ng-href="/#/orders"><i class="fa fa-history"></i> See all</a></li>
                                <li><a class="menu-navbar" data-ng-href="/#/orders"><i class="fa fa-history"></i> Clear all</a></li>
                            </ul>
                        </li>
                    </ul>
                    
                    <ul class="nav navbar-nav navbar-right">
                        <li class="dropdown">
                          <a class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><%=username%>&nbsp;&nbsp;&nbsp;<img src="<%=image%>" class="myimage" style="border-radius:15px;"height="30px" width="30px"></a>
