                                <li><a class="menu-navbar" data-ng-href="/#/orders"><i class="fa fa-history"></i> Meus pedidos</a></li>
                                <li><a class="menu-navbar" data-ng-href="/#/account"><i class="fa fa-user"></i> Minha conta</a></li>
                                <li role="separator" class="divider"></li>
                                <li><a class="menu-navbar" data-ng-href="/auth/logout/"><i class="fa fa-power-off"></i> Logout</a></li>
                            </ul>
                        </li>
                        <% } else {  %>
                    <ul class="nav navbar-nav navbar-right">
                            <li class="dropdown">
                                <a class="dropdown-toggle" data-ng-href="#/auth/" >Login</a>
                            </li>
                        <% }  %>
                    </ul>

                </div>

            </nav>
        </div>
        <% if( logged != 1 && logged ){ %> 
            <% include menu %>
            <div class="col-xs-10 col-sm-10 col-md-10 col-lg-10" data-ng-view></div>
        <%}else{%>
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12" data-ng-view></div>
